{
    "problemMatcher": [
        {
            "owner": "dotnet-file",
            "pattern": [
                {
                    "regexp": "^(.+)\\((\\d+).+\\):\\s(\\w+)\\s(.+)\\:\\s(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "dotnet-run",
            "pattern": [
                {
                    "regexp": "^.+\\s\\:\\s(\\w+)\\s(.*)\\:\\s(.*)$",
                    "severity": 1,
                    "code": 2,
                    "message": 3
                }
            ]
        }
    ]
}
